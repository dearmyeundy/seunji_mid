{
    "name": "Streamlit Motion Simulation",
    "dockerFile": "Dockerfile",
    "context": "..",
    "settings": {
        "python.pythonPath": "/usr/local/bin/python"
    },
    "extensions": [
        "ms-python.python",
        "ms-toolsai.jupyter",
        "streamelements.streamlit"
    ],
    "postCreateCommand": "pip install -r requirements.txt",
    "remoteUser": "vscode"
}